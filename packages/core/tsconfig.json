{
    "extends": "../../tsconfig.json",

    "include": ["src/**/*"],

    "compilerOptions": {
        "baseUrl": "./src"
    },

    "typedocOptions": {
        "mode": "modules",
        "target": "ES6",
        "out": "docs",
        "excludeNotExported": true,
        "excludePrivate": true,
        "stripInternal": "true"
    }
}
